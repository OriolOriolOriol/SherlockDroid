<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sherlockdroid" xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.READ_LOGS"  tools:ignore="ProtectedPermissions"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SherlockDroid">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".tools.ExportedActivity"></activity>
        <activity android:name=".tools.ExportedReceiver"></activity>
        <activity android:name=".tools.ExportedService"></activity>
        <activity android:name=".tools.ExportedContent"></activity>
        <activity android:name=".tools.ContentURI"></activity>
        <activity android:name=".tools.ExportedContentPathTraversal"></activity>
    </application>

</manifest>